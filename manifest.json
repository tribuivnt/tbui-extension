{
    "manifest_version": 2,

    "name": "TBui Extension",
    "description": "TBui Extension for Browsers",
    "version": "0.0.3",
    "icons": {  "16": "/icons/icon_16x16.png",
                "48": "/icons/icon_48x48.png",
                "128": "/icons/icon_128x128.png" },
    "browser_action": {
        "default_icon": "/icons/icon_16x16.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}